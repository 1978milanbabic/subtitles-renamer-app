{
  "name": "electron-test",
  "version": "0.1.1",
  "description": "Subtitle Renamer",
  "main": "main.js",
  "keywords": [
    "electron"
  ],
  "license": "ISC",
  "author": "Milan Babic",
  "scripts": {
    "dev": "concurrently -k \"BROWSER=none npm start\" \"npm:electron\"",
    "electron": "wait-on tcp:1234 && electron-forge start",
    "start": "parcel public/index.html",
    "build": "parcel build public/index.html",
    "package": "parcel build public/index.html && electron-forge package",
    "make": "parcel build public/index.html && electron-forge make"
  },
  "dependencies": {
    "electron-is-dev": "^2.0.0",
    "electron-squirrel-startup": "^1.0.0",
    "fs-extra": "^10.0.0",
    "lodash": "^4.17.21",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "semantic-ui-css": "^2.4.1",
    "semantic-ui-react": "^2.0.4"
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.61",
    "@electron-forge/maker-deb": "^6.0.0-beta.61",
    "@electron-forge/maker-rpm": "^6.0.0-beta.61",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.61",
    "@electron-forge/maker-zip": "^6.0.0-beta.61",
    "@parcel/packager-raw-url": "^2.0.1",
    "@parcel/transformer-webmanifest": "^2.0.1",
    "concurrently": "^6.5.1",
    "electron": "^16.0.4",
    "electron-devtools-installer": "^3.2.0",
    "parcel": "^2.0.1",
    "wait-on": "^6.0.0"
  }
}
